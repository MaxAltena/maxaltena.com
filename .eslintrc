{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "eslint-config-maxaltena"],
	"globals": {
		"require": "writable",
		"module": "writable"
	},
	"plugins": ["react", "prettier"]
}
