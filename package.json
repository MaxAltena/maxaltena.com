		"react-dom": "^18.2.0",
		"tailwind-merge": "^1.14.0",
		"zod": "^3.22.3"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^13.4.19",
		"@total-typescript/ts-reset": "^0.5.1",
		"@trivago/prettier-plugin-sort-imports": "^4.2.0",
		"@types/eslint": "^8.44.2",
