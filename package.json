		"gatsby-plugin-sitemap": "^4.7.0",
		"gatsby-plugin-web-font-loader": "^1.0.4",
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-draggable": "^4.4.3",
		"react-helmet": "^6.1.0",
		"react-measure": "^2.5.2"
	},
	"devDependencies": {
		"@maxaltena/eslint-config-maxaltena": "^1.1.2",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"node-sass": "^6.0.1",
		"prettier": "^2.3.2",
		"snyk": "^1.680.0"
	},
	"engines": {
		"node": ">= 8.17.0",
		"npm": ">= 6.12.1",
		"yarn": ">= 1.19.1"
	},
	"snyk": true
}
