{
	"name": "maxaltena.com",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build:production",
		"build:development": "cross-env NODE_ENV='development' npm run build",
		"build:production": "cross-env NODE_ENV='production' npm run build",
		"build:test": "cross-env NODE_ENV='test' npm run build",
		"dev": "next dev",
		"dev:debug": "cross-env NODE_OPTIONS='--inspect' npm run dev",
		"lint": "npm run lint:typescript && npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint . --ext .js,.jsx,.mjs,.cjs,.ts,.tsx",
		"lint:eslint:fix": "npm run lint:eslint -- --fix",
		"lint:fix": "npm run lint:typescript && npm run lint:eslint:fix && npm run lint:prettier:fix",
		"lint:prettier": "prettier . --check --ignore-unknown",
		"lint:prettier:fix": "npm run lint:prettier -- --write",
		"lint:typescript": "tsc --noEmit",
		"sort-package-json": "sort-package-json",
		"start": "next start"
	},
