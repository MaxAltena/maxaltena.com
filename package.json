		"gatsby-plugin-google-tagmanager": "^3.11.0",
		"gatsby-plugin-html-attributes": "^1.0.5",
		"gatsby-plugin-humans-txt": "^1.1.4",
		"gatsby-plugin-manifest": "^3.11.0",
