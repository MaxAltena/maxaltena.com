{
  "name": "maxaltena",
  "description": "Website of Max Altena",
  "version": "0.0.1",
  "keywords": [
    "react",
    "app",
    "max",
    "altena",
    "max altena",
    "portfolio",
    "website",
    "resume",
    "projects"
  ],
  "private": true,
  "author": "Max Altena <Max@MaxAltena.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaxAltena/maxaltena.com.git"
  },
  "homepage": "https://maxaltena.com/",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.6.3",
    "react-burger-menu": "^2.6.1",
    "react-dom": "^16.6.3",
    "react-lazyload": "^2.3.0",
    "react-parallax": "^2.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-scroll": "^1.7.10"
  },
  "devDependencies": {
    "ftp-deploy": "^2.2.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --use-polling --polling-interval 1000",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
