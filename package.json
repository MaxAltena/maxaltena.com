{
	"name": "maxaltena.com",
	"version": "1.0.1",
	"private": false,
	"description": "The website of Max Altena",
	"keywords": [
		"max",
		"altena",
		"max altena",
		"maxaltena",
		"portfolio",
		"website",
		"resume",
		"projects"
	],
	"homepage": "https://maxaltena.com/",
	"bugs": {
		"url": "https://github.com/MaxAltena/maxaltena.com/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MaxAltena/maxaltena.com.git"
	},
	"license": "MIT",
	"author": "Max Altena <max@maxaltena.com> (https://maxaltena.com/)",
	"scripts": {
		"build": "gatsby build",
		"dev": "gatsby develop --port 8008",
		"prettier": "prettier **/*",
		"prettier:fix": "prettier **/* --write",
		"eslint": "eslint .",
		"eslint:fix": "eslint . --fix",
		"serve": "gatsby serve",
		"start": "npm run dev",
		"test": "echo \"Error: test not specified\" && exit 0",
		"snyk-protect": "snyk protect",
		"prepare": "npm run snyk-protect"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "npm run prettier:fix",
		"*.js": "npm run eslint:fix"
	},
	"browserslist": [
		"last 1 version",
		"> 0.25%",
		"maintained node versions",
		"not dead"
	],
	"dependencies": {
		"axios": "^0.21.1",
		"baffle": "^0.3.6",
