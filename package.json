		"prettier-plugin-tailwindcss": "^0.5.3",
		"sort-package-json": "^2.5.1",
		"tailwindcss": "^3.3.3",
		"tailwindcss-animate": "^1.0.6",
		"typescript": "^5.2.2"
	},
	"packageManager": "pnpm@8.6.5",
